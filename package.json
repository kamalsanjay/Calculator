{
  "name": "calculator-website",
  "version": "1.0.0",
  "description": "Professional calculator website with 300+ tools",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "css:build": "sass assets/scss:assets/css --style compressed",
    "css:watch": "sass assets/scss:assets/css --watch",
    "js:minify": "terser assets/js/*.js -o assets/js/bundle.min.js",
    "lint": "eslint assets/js/**/*.js",
    "format": "prettier --write 'assets/**/*.{js,css,json}'"
  },
  "keywords": [
    "calculator",
    "online",
    "tools",
    "math",
    "financial"
  ],
  "author": "Calculator Team",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.4.0",
    "jquery": "^3.7.1",
    "bootstrap": "^5.3.2",
    "@fortawesome/fontawesome-free": "^6.5.1"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "sass": "^1.69.5",
    "terser": "^5.26.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "babel-loader": "^9.1.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}